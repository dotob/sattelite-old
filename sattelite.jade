head
  title sattelite

body
  +nav
  +main

template(name="nav")
  nav.navbar.navbar-default.navbar-fixed-top(role="navigation")
    .container-fluid
      .navbar-header
        button.navbar-toggle(type="button" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1")
          span.sr-only Toggle navigation
          span.icon-bar
          span.icon-bar
        a.navbar-brand(href="#") sattelite
      .collapse.navbar-collapse#bs-example-navbar-collapse-1
        ul.nav.navbar-nav
          li
            a(href="#") Link
          li
            a(href="#") Link

template(name="main")
  .container
    .row
      .col-md-12
        +dishes

template(name="dishes")
  div.dish
    each dishes
      .row
        .col-md-2
          .order_number #{order_number}
        .col-md-8
          .name #{name}
        .col-md-2
          .price #{price}
